{
   "action": {
      "default_icon": "icon-128.png",
      "default_popup": "popup.html",
      "default_title": "Chessvision.ai Scan"
   },
   "author": "Pawel Kacprzak",
   "background": {
      "service_worker": "background.bundle.js"
   },
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "description": "Analyze chess positions from any website, book, and video in Chrome",
   "externally_connectable": {
      "matches": [ "https://*.chessvision.ai/*" ]
   },
   "host_permissions": [ "*://app.chessvision.ai/*" ],
   "icons": {
      "128": "icon-128.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Chessvision.ai Chess Position Scanner",
   "optional_host_permissions": [ "*://www.chess.com/*", "*://www.youtube.com/*", "*://lichess.org/*", "*://twitter.com/*" ],
   "options_page": "options.html",
   "permissions": [ "activeTab", "storage", "scripting" ],
   "short_name": "Chessvision",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.4.1",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "content.styles.css", "icon-128.png", "logo_sign.png", "bK.svg", "wK.svg", "fen_observers/*", "placeholders/*", "checkerboard.svg", "chessable.svg", "videos.svg" ]
   } ]
}
